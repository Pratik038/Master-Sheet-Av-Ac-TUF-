,0,0,0,0,0,0,0,0,0};
    vector<vector<int> >res=threeSum(v,0);
    sort(res.begin(),res.end());
    res.erase(unique(res.begin(),res.end()),res.end());
    for(int i=0;i<res.size();i++)
    {
        for(int j=0;j<res[i].size();j++)
            cout<<res[i][j]<<" ";
        cout<<"\n";
}
    return 0;
}


