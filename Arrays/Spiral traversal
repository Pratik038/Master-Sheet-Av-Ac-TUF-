#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int m,n;
        cin>>m>>n;
        int arr[m][n];
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<n;j++)
                cin>>arr[i][j]; 
        }
        // direction -> 0(L->r) 1(t->d) 2(r->L) 3(d->t)
        int dir=0;
        // 4 pointers pointing to 4 boundries
        int l=0,r=m-1,t=0,d=n-1;
        while(l<=r && t<=d )
        {
            if(dir==0)
            {
                for(int i=l;i<=r;i++)
                    cout<<arr[t][i]<<" ";
                t++;    
            }
            else if(dir==1)
            {
                for(int i=t;i<=d;i++)
                    cout<<arr[i][r]<<" ";
                r--;    
            }
            else  if(dir==2)
            {
                for(int i=r;i>=l;i--)
                    cout<<arr[d][i]<<" ";
                d--;     
            }
            else  if(dir==3)
            {
                for(int i=d;i>=t;i--)
                    cout<<arr[i][l]<<" ";
                l++; 
            }
            dir=(dir+1)%4;
        }
        cout<<"\n";
        
    }
    return 0;
}
