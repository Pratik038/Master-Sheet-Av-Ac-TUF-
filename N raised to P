//Recusive function to calculate n raised to power of p
#include<bits/stdc++.h>
using namespace std;
int pow(int n,int p)
{
    if(p==0) return 1;
    return n*pow(n,p-1);
}
int main()
{
    int n,p;
    cin>>n>>p;
    cout<<pow(n,p);
    return 0;
}


